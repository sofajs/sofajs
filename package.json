{
  "name": "sofajs",
  "version": "0.0.10",
  "description": "couchdb interface builder",
  "main": "lib/index.js",
  "directories": {
    "example": "example",
    "test": "test"
  },
  "dependencies": {
    "async": "1.x.x",
    "bcrypt": "0.x.x",
    "bluebird": "2.x.x",
    "hoek": "2.x.x",
    "inert": "3.x.x",
    "items": "1.x.x",
    "joi": "^6.7.x",
    "marked": "^0.3.5",
    "nano": "6.x.x"
  },
  "devDependencies": {
    "code": "1.x.x",
    "docos": "0.0.x",
    "lab": "5.x.x"
  },
  "scripts": {
    "start": "node example/index",
    "test": "node node_modules/lab/bin/lab -a code -t 100 -L -v",
    "test-cov-html": "node node_modules/lab/bin/lab -a code -r html -o coverage.html",
    "docos": "node sample/lib/docos",
    "load": "node sample/lib/load"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sofajs/sofajs.git"
  },
  "keywords": [
    "couchdb",
    "sofajs",
    "interface",
    "nano"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/sofajs/sofajs/issues"
  },
  "homepage": "https://github.com/sofajs/sofajs"
}
